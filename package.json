{
  "name": "react-meet-up",
  "description": "Deep dive in ReactJS",
  "keywords": [
    "javascript",
    "es6"
  ],
  "author": "OnSeo",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/onseopublic/react-meet-up"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm-run-all --parallel start:mockserver start:app",
    "start:mockserver": "node mockServer.js",
    "start:app": "webpack-dev-server --mode development",
    "test": "jest",
    "precommit": "lint-staged",
    "prepush": "npm run test"
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-jest": "23.6.0",
    "babel-runtime": "6.26.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "identity-obj-proxy": "3.0.0",
    "immutable": "3.8.2",
    "json-server": "0.14.0",
    "npm-run-all": "4.1.3",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-redux": "5.0.7",
    "recompose": "0.30.0",
    "redux": "4.0.0",
    "redux-actions": "2.6.1",
    "redux-immutable": "4.0.0",
    "redux-promise-middleware": "5.1.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-airbnb": "2.6.0",
    "clean-webpack-plugin": "0.1.19",
    "directory-named-webpack-plugin": "4.0.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^1.1.1",
    "jest": "23.6.0",
    "jest-webpack-resolver": "0.3.0",
    "lint-staged": "7.2.2",
    "prettier-eslint-cli": "4.7.1",
    "webpack": "4.18.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-manifest-plugin": "2.0.4",
    "yargs": "12.0.2"
  },
  "lint-staged": {
    "*.{js,json,jsx}": [
      "prettier --print-width 120 --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jestWebpackResolver": {
    "silent": true,
    "webpackConfig": "./webpack.config.js"
  }
}
